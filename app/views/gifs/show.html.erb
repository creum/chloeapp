<div class="container">
  <div class="row">
    <div class="col-md-6">
      <img src="<%= @gif.image_url %>"/>
    </div>
    <div class="col-md-6">
      <h1><%= @gif.title %></h1>

      <h2> Description: </h2>
      <h4><%= @gif.description %></h4>
        <%= link_to 'Edit', edit_gif_path(@gif) %> |
        <%= link_to 'Back', gifs_path %>
    </div>
  </div>
</div>


<p id="notice"><%= notice %></p>

