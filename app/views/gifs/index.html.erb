<div class="jumbotron">
  <h1>Adorable Baby Animals</h1>
  <p>Dog, Ducky, Koala by Chloe Reum</p>
  <p><a class="btn btn-primary btn-lg" role="button">Learn more</a></p>
</div>

<% @gifs.each do |gif| %>
      <div class="panel panel-default">
        <div class="panel-heading"><%= gif.title %></div>
        <div class="panel-body">
          <img src="<%= gif.image_url %>">
        </div>
        <div class="panel-footer"><%= gif.description %></div>
      </div>
<% end %>

